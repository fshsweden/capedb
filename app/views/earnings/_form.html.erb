<%= simple_form_for(@earning) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :belongs_to %>
    <%= f.input :date_of_entry %>
    <%= f.input :year %>
    <%= f.input :profit %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
